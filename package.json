{
  "name": "n8n-nodes-superssh",
  "version": "1.0.1",
  "description": "Enhanced n8n SSH node with advanced credential management and specialized support for Cisco and Aruba network devices",
  "keywords": [
    "n8n-community-node-package",
    "ssh",
    "scp",
    "sftp",
    "custom-credentials",
    "enhanced-security",
    "network-devices",
    "cisco",
    "aruba"
  ],
  "license": "MIT",
  "homepage": "https://github.com/timsemisch/SuperSSH",
  "author": {
    "name": "Tim Semisch",
    "email": "tim.semisch@kupper-computer.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timsemisch/SuperSSH.git"
  },
  "engines": {
    "node": ">=18.10",
    "npm": ">=9.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-files",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "copy-files": "node scripts/copy-files.js"
  },
  "files": [
    "dist",
    "nodes",
    "credentials",
    "utils",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SuperSshCredentials.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SuperSsh/SuperSsh.node.js",
      "dist/nodes/SshTemplateParser/SshTemplateParser.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/fs-extra": "^11.0.1",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "node-ssh": "^13.1.0",
    "tmp-promise": "^3.0.3",
    "fs-extra": "^11.1.1"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
