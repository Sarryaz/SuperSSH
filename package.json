{
	"name": "n8n-nodes-ssh-nexus",
	"version": "1.0.1",
	"description": "🚀 SSH Nexus - The ultimate SSH automation hub for n8n! Features intelligent connection health monitoring, smart network discovery tools, advanced security auditing, performance benchmarking, and comprehensive device support. Built with ❤️ by Sarryaz for the automation community!",
	"keywords": [
		"n8n",
		"n8n-node",
		"n8n-community-node-package",
		"ssh",
		"superssh",
		"network-automation",
		"network-devices",
		"cisco",
		"aruba",
		"connection-health",
		"network-discovery",
		"security-audit",
		"performance-benchmark",
		"smart-tools",
		"dynamic-parameters",
		"cipher-selection",
		"configuration-management",
		"ai-agent",
		"automation",
		"devops"
	],
	"license": "MIT",
	"homepage": "https://github.com/Sarryaz/SuperSSH",
	"author": {
		"name": "Sarryaz",
		"email": "sarryaz@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Sarryaz/SuperSSH.git"
	},
	"engines": {
		"node": ">=18.10",
		"npm": ">=9.0.0"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && npm run copy-files",
		"dev": "tsc --watch",
		"prepublishOnly": "npm run build",
		"copy-files": "node scripts/copy-files.js"
	},
	"files": [
		"dist",
		"nodes",
		"credentials",
		"utils",
		"examples",
		"README.md",
		"LICENSE"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/SuperSshCredentials.credentials.js"
		],
		"nodes": [
			"dist/nodes/SuperSsh/SuperSsh.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^22.13.10",
		"typescript": "^5.5.3"
	},
	"dependencies": {
		"node-ssh": "^13.1.0",
		"tmp-promise": "^3.0.3"
	},
	"peerDependencies": {
		"n8n-workflow": ">=1.17.1"
	},
	"aiTool": {
		"usableAsTool": true,
		"capabilities": [
			"ssh-command",
			"ssh-file-upload",
			"ssh-file-download",
			"network-device-ops"
		]
	}
}
