{
  "name": "n8n-nodes-ssh-nexus",
  "version": "0.1.0",
  "description": "Enhanced SSH operations with TextFSM template parsing capabilities for network device automation",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "ssh",
    "network",
    "automation",
    "textfsm",
    "parsing",
    "aruba",
    "cisco",
    "juniper"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Tim Semisch",
    "email": "tim.semisch@kupper-computer.de"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/n8n-nodes-ssh-nexus.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json && node scripts/copy-files.js",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials package.json",
    "lint": "eslint nodes credentials package.json",
    		"prepublishOnly": "npm run build",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/SuperSsh/SuperSsh.node.js",
      "dist/nodes/SshTemplateParser/SshTemplateParser.node.js"
    ],
    "credentials": [
      "dist/credentials/SuperSshCredentials.credentials.js"
    ]
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.9",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "node-ssh": "^13.1.0",
    "tmp-promise": "^3.0.2"
  }
}
