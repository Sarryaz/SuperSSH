{
  "name": "n8n-nodes-sshtemplateparser",
  "version": "0.1.0",
  "description": "n8n community node: SSH Template Parser with TextFSM-like engine",
  "author": "Community",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && node scripts/copy-files.js",
    "dev": "tsc -w -p tsconfig.json",
    "prepare": "npm run build"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/SshTemplateParser/SshTemplateParser.node.js",
      "dist/nodes/SuperSsh/SuperSsh.node.js"
    ],
    "credentials": [
      "dist/credentials/SuperSshCredentials.credentials.js"
    ]
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "node-ssh": "^13.1.0",
    "path": "^0.12.7",
    "tmp-promise": "^3.0.3",
    "n8n-core": "^1.61.0",
    "n8n-workflow": "^1.61.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.11.19",
    "typescript": "^5.4.0"
  }
}
