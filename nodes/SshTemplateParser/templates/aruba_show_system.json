{
  "id": "aruba_show_system_v1",
  "name": "Aruba Show System",
  "vendor": "aruba",
  "deviceOs": "aruba_os",
  "command": "show system",
  "description": "Parse system information for Aruba OS",
  "states": [
    {
      "name": "start",
      "patterns": [
        {
          "regex": "^System\\s+Name\\s*:\\s*(?<SYSTEM_NAME>\\S+)",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^System\\s+Description\\s*:\\s*(?<SYSTEM_DESCRIPTION>.+)",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^System\\s+Contact\\s*:\\s*(?<SYSTEM_CONTACT>.+)",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^System\\s+Location\\s*:\\s*(?<SYSTEM_LOCATION>.+)",
          "actions": [{ "type": "emit" }],
          "transition": { "to": "start" }
        }
      ]
    }
  ],
  "metadata": {
    "created": "2025-08-23T00:00:00.000Z",
    "updated": "2025-08-23T00:00:00.000Z",
    "version": "1.0.0",
    "author": "community",
    "tested": false
  }
}
