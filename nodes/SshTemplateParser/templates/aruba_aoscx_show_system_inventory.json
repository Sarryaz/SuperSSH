{
  "id": "aruba_aoscx_show_system_inventory_v1",
  "name": "AOS-CX Show System Inventory",
  "vendor": "aruba",
  "deviceOs": "aruba_aoscx",
  "command": "show system inventory",
  "description": "Parse system inventory for AOS-CX",
  "states": [
    {
      "name": "start",
      "patterns": [
        {
          "regex": "^Type.*",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^\\s+Number\\s+Number\\s+Version.*",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^----.*",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^(?<TYPE>.{20})\\s(?<LOCATION>.{8})\\s(?<PRODUCT_NUM>.{10})\\s(?<DESC>.{50})\\s(?<SERIAL_NUM>.{15})\\s(?<HARDWARE_VER>\\S+)",
          "actions": [{ "type": "emit" }],
          "transition": { "to": "self" }
        }
      ]
    }
  ],
  "metadata": {
    "created": "2025-08-23T00:00:00.000Z",
    "updated": "2025-08-23T00:00:00.000Z",
    "version": "1.0.0",
    "author": "community",
    "tested": false
  }
}
