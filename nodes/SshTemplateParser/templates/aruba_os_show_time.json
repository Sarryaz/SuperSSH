{
  "id": "aruba_os_show_time_v1",
  "name": "Aruba OS Show Time",
  "vendor": "aruba",
  "deviceOs": "aruba_os",
  "command": "show time",
  "description": "Parse system time for Aruba OS",
  "states": [
    {
      "name": "start",
      "patterns": [
        {
          "regex": "^(?<DAYWEEK>\\w\\w\\w)\\s(?<MONTH>\\w\\w\\w)\\s(?<DAY>\\d+)\\s(?<TIME>\\d+:\\d+:\\d+)\\s(?<YEAR>\\d+)",
          "actions": [{ "type": "emit" }],
          "transition": { "to": "self" }
        }
      ]
    }
  ],
  "metadata": {
    "created": "2025-08-23T00:00:00.000Z",
