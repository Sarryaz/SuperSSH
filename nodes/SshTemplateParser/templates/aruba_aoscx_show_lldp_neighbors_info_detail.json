{
  "id": "aruba_aoscx_show_lldp_neighbors_info_detail_v1",
  "name": "AOS-CX Show LLDP Neighbors Info Detail",
  "vendor": "aruba",
  "deviceOs": "aruba_aoscx",
  "command": "show lldp neighbors-info detail",
  "description": "Parse LLDP neighbors info detail for AOS-CX",
  "states": [
    {
      "name": "start",
      "patterns": [
        {
          "regex": "^\\s*CDP\\s+neighbors\\s+information\\s*",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^\\s*Port\\s*:\\s*(?<LOCAL_INTERFACE>.+?)\\s*$$",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^\\s*Device\\s+ID\\s*:\\s*(?<CHASSIS_ID>.+?)\\s*$$",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^\\s*Address\\s+Type\\s*$",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^\\s*Address\\s*:\\s*(?<MGMT_ADDRESS>.+?)\\s*$$",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^\\s*Platform\\s*:\\s*(?<PLATFORM>.+?)\\s*$$",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^\\s*Capability\\s*:\\s*(?<CAPABILITIES>.+?)\\s*$$",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^\\s*Device\\s+Port\\s*:\\s*(?<NEIGHBOR_INTERFACE>.+?)\\s*$$",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^\\s*Version\\s*:\\s*(?<NEIGHBOR_DESCRIPTION>.+?)\\s*$$",
          "actions": [{ "type": "emit" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^\\s*$$",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        },
        {
          "regex": "^\\s*----\\s*$",
          "actions": [{ "type": "next", "to": "start" }],
          "transition": { "to": "start" }
        }
      ]
    }
  ],
  "metadata": {
    "created": "2025-08-23T00:00:00.000Z",
